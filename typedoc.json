{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/**/*.ts"],
  "exclude": ["src/utils.ts", "src/**/*.spec.ts"],
  "out": "docs",
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "includeVersion": true,
  "hideGenerator": true,
  "categorizeByGroup": true,
  "sort": ["source-order"]
}
